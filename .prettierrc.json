{
  "arrowParens": "always",
  "bracketSpacing": true,
  "configPath": "",
  "documentSelectors": [],
  "embeddedLanguageFormatting": "auto",
  "enable": true,
  "enableDebugLogs": false,
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "css",
  "ignorePath": ".prettierignore",
  "insertPragma": false,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": true,
  "prettierPath": "",
  "printWidth": 90,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "requireConfig": false,
  "requirePragma": false,
  "resolveGlobalModules": false,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useEditorConfig": true,
  "useTabs": false,
  "vueIndentScriptAndStyle": true,
  "withNodeModules": false
